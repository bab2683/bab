  },
  "version": "0.0.0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bab2683/bab/issues"
  },
  "scripts": {
    "cz": "git-cz",
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve -o --port=4300",
    "build": "ng build",
    "test": "ng test -u",
    "test:cov": "ng test --codeCoverage=true",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prepush": "nx affected:lint",
    "lib-publish": "node ./scripts/publish.js",
    "testing": "node ./scripts/test.js"
  },
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^11.0.5",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@ngrx/effects": "^8.3.0",
    "@ngrx/router-store": "^8.3.0",
    "@ngrx/store": "^8.3.0",
    "@nrwl/angular": "8.4.13",
    "core-js": "^2.5.4",
    "firebase": "^6.6.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.1",
    "@angular-devkit/build-ng-packagr": "~0.801.1",
    "@angular/cli": "8.1.1",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@nrwl/cypress": "8.4.13",
    "@nrwl/jest": "8.4.13",
    "@nrwl/workspace": "8.4.13",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "commitizen": "^4.0.3",
    "cypress": "3.4.1",
    "cz-conventional-changelog": "git+https://github.com/Ryanair/cz-conventional-changelog.git",
    "dotenv": "6.2.0",
    "eslint": "6.1.0",
    "husky": "^3.0.5",
    "jest": "24.1.0",
    "jest-preset-angular": "7.0.0",
    "lint-staged": "^9.2.5",
    "ng-packagr": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.16.4",
    "rxjs-tslint-rules": "^4.25.0",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prepush"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint",
      "prettier --single-quote --write",
      "git add"
    ],
    "*.scss": [
      "stylelint"
    ],
    "*.html": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "scopeList": [
        "auth",
        "test-utils"
      ]
    }
  }
}
